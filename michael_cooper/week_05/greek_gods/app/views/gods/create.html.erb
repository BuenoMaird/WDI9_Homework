<form action="/gods" method="post">
  <label for="name">Name:</label>
  <input type="text" id="name" name="god[name]">
  <label for="image">Image:</label>
  <input type="url" id="image" name="god[image]">
  <label for="description">Description:</label>
  <input type="text" id="description" name="god[description]">
  <label for="faction">Alignment:</label>
  <select id="faction" name="god[faction]">
    <option value="None">None</option>
    <option value="Olympian">Olympian</option>
    <option value="Titans">Titans</option>
    <option value="Oceanic">Oceanic</option>
  </select>
  <!-- <input type="text" id="faction" name="god[faction]"> -->
  <label for="symbol">Symbol:</label>
  <input type="text" id="symbol" name="god[symbol]">
  <label for="children">Children:</label>
  <input type="text" id="children" name="god[children]">
  <br>
  <button>Unleash a New God!</button>
</form>